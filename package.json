{
  "name": "reviews",
  "version": "1.0.0",
  "description": "reviews module for appetizer-aperitif",
  "private": true,
  "scripts": {
    "react-dev": "webpack -d --watch",
    "test": "jest --coverage --colors",
    "server": "nodemon server/index.js",
    "seed": "node database/seed.js",
    "start:docker": "npm run seed && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appetizer-aperitif/reviews.git"
  },
  "keywords": [
    "yelp",
    "clone",
    "food",
    "drink",
    "reviews"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "author": "Lee Graham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appetizer-aperitif/reviews/issues"
  },
  "homepage": "https://github.com/appetizer-aperitif/reviews#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "jest": "^24.8.0",
    "live-server": "^1.2.1",
    "lodash": "^4.17.19",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongo": "^0.1.0",
    "mongoose": "^5.6.3",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
